import java.util.Scanner;
public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a;
	  	  a = sc.nextInt();

		for(int i=1; i<=a; i++) {    // j가 줄, i가 가로
			for(int j=a;j>i;j--) {
				System.out.print(" ");
			}
			for(int j=1; j<=i; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
    }
}
